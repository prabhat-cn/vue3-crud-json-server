{
  "name": "vue-crud-json-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && yarn build",
    "gbuild": "concurrently \"yarn serve\" \"json-server --watch data/db.json --port 8085\" \"vue-cli-service build\"",
    "dev": "concurrently \"yarn serve\" \"json-server --watch data/db.json --port 8085\""
  },
  "dependencies": {
    "axios": "^0.24.0",
    "concurrently": "^7.0.0",
    "core-js": "^3.6.5",
    "json-server": "^0.17.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0"
  }
}
